{
  "manifest_version": 2,

  "name": "Soundcloud Queue",
  "description": "Temoorary des",
  "version": "1",

 "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
      "matches":[
        "http://*/*",
        "https://*/*"
      ],
      "css": ["content/content.css"],
      "js": ["js/sdk.js", "js/jquery.min.js", "content/content_script.js"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": ["js/jquery.min.js", "background/background.js"],
    "persistent" : false
  },
  "web_accessible_resources": [
    "assets/*",
    "templates/*"
  ],
  "permissions": [
        "http://api.soundcloud.com/*",
        "activeTab",
        "tabs",
        "http://*/*",
        "background"
  ]
}
