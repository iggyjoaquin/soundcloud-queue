{
  "manifest_version": 2,

  "name": "soundcloud queue",
  "description": "Temoorary description",
  "version": "1",

 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["sdk.js", "jquery.min.js", "popup.js", "main.js"]
  },
  "permissions": [
        "http://api.soundcloud.com/*"
  ]
}
